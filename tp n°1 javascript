<!DOCTYPE html>
<html>
    <head>
        <title> TODO proporcione un título </title>
        <meta charset = "UTF-8">
        <meta name = "viewport" content = "width = device-width, initial-scale = 1.0">
    </head>
    <body>
<script>    
    let i;
    let buc;
    for (i = 1; i <= 30; i++) {
        for (buc = 0; buc < i; buc++) {
            document.write(i);
        }
        document.write("<br>");
    }
</script>
</body>
</html>
6)
<!DOCTYPE html>
<html>
    <head>
        <title> TODO proporcione un título </title>
        <meta charset = "UTF-8">
        <meta name = "viewport" content = "width = device-width, initial-scale = 1.0">
    </head>
    <body>
<script>
    
let num = prompt("ingresar un numero para las repeticiones del 1 al 30");

if (parseInt(num) == num) {
    
    if (num > 0 && num <= 30) {
        let n;
        let buc;

        for (n = num; n >= 1; n--) {

            for (buc = n; buc >= 1; buc--) {
                document.write(n);
            }
            document.write("<br>");
        }
    } 
    else if(num>30){
    alert("ingresar un numero del 1 al 30");
    }
}
else {
  alert("por favor ingresar un numero");
}
</script>
</body>
</html>
